  <section id="about-us" class="padding-large">
    <div class="container-fluid padding-side" data-aos="fade-up">
      <center><p style="font-size: 60px;">Download File</p>
      </center>
      <br>
      <center><p style="font-size: 30px;">File ID: <%= ID %></p></center>
      <div class="widget-centered">
        <form action="/" method="post">
          <div class="inlinin">
            <div>
              <input type="hidden" name="fileID" value="<%= ID %>">
              <input class="enterid" type="password" placeholder="Password" name="password" />
            </div>
          </div>
          <br><br>
          <button type="submit" class="recievebtn">Download</button>
        </form>
        <a href="#chat">
          <button class="tios">Chat with the receivers</button>
        </a>
      </div>
      <br>
    </div>
  </section>
  <section class="msger" data-aos="fade-up" id="chat">
    <header class="msger-header">
        <div class="msger-header-title">
          <p class="sf-indicator" title="XPR-WSS is unmonitored. Use responsibly.">
            <span id="statusdot" class="status-dot status-green"></span>
            XPR-WSS: <i id="sockdis" class="text-danger">Disconnected</i>
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-info-circle" viewBox="0 0 16 16" title="XPR-WSS is unmonitored.">
              <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14m0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16"/>
              <path d="m8.93 6.588-2.29.287-.082.38.45.083c.294.07.352.176.288.469l-.738 3.468c-.194.897.105 1.319.808 1.319.545 0 1.178-.252 1.465-.598l.088-.416c-.2.176-.492.246-.686.246-.275 0-.375-.193-.304-.533zM9 4.5a1 1 0 1 1-2 0 1 1 0 0 1 2 0"/>
            </svg>
          </p>
        </div>
        <div class="msger-header-options">
            <span><i class="fas fa-cog"></i></span>
        </div>
    </header>
    <div id="overlay" style="<%= wss ? 'display: none;' : '' %>">
      <button id="startChatBtn">Start Chat</button>
  </div>
    <main class="msger-chat">
        <!-- Chat messages go here -->
    </main>

    <form class="msger-inputarea" javascript:void(0)>
        <input type="text" class="msger-input" placeholder="Enter your message...">
        <button type="submit" class="msger-send-btn btn-11">Send</button>
    </form>
</section>

